    "x = float(input(\"Give a number:\"))\n",
    "\n",
    "import math\n",
    "x = math.ceil(x)\n",
    "print(0, end = '')\n",
    "\n",
    "for i in range(0,x):\n",
    "    if (round(i/3,0) == i/3):\n",
    "        continue\n",
    "    print(\", \"+ str(i), end ='')\n",
    "    i += 1\n",
    "    \n",
    "print(\".\")"
