   "source": [
    "x = float(input(\"Give a number:\"))\n",
    "\n",
    "import math\n",
    "x = math.ceil(x)\n",
    "\n",
    "F0 = 0\n",
    "F1 = 1\n",
    "\n",
    "while F1 < x:\n",
    "  if F0 == 0: print(0, end = ', ')\n",
    "  if F0+F1 < x: print(F1, end = ', ')\n",
    "  else: print(F1, end = '. ')\n",
    "  F0,F1 = F1,F0+F1"
   ]
